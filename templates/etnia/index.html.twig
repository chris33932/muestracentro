    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etnium in etnias %}
            <tr>
                <td>{{ etnium.id }}</td>
                <td>{{ etnium.descripcion }}</td>
                <td>
                    <a href="{{ path('etnia_show', {'id': etnium.id}) }}">show</a>
