{% extends 'base.html.twig' %}

{% block title %}{{ parent() }}{% endblock %}

{% block body %}{{ parent() }}
<br>

<div class="container">
   <h2>Sexo categorias</h2>
  <p>Lista de categorias para la variable sexo:</p>    
   <br>        
  <table class="table">
    <thead class="thead-light">
 
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sexo in sexos %}
            <tr>
                <td>{{ sexo.id }}</td>
                <td>{{ sexo.descripcion }}</td>
                <td>
                    <a href="{{ path('sexo_show', {'id': sexo.id}) }}">show</a>
                    <a href="{{ path('sexo_edit', {'id': sexo.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sexo_new') }}">Create new</a>
{% endblock %}
