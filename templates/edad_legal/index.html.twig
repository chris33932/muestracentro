{% extends 'base.html.twig' %}

{% block title %}EdadLegal index{% endblock %}

{% block body %}
    <h1>EdadLegal index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for edad_legal in edad_legals %}
            <tr>
                <td>{{ edad_legal.id }}</td>
                <td>{{ edad_legal.descripcion }}</td>
                <td>
                    <a href="{{ path('edad_legal_show', {'id': edad_legal.id}) }}">show</a>
                    <a href="{{ path('edad_legal_edit', {'id': edad_legal.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('edad_legal_new') }}">Create new</a>
{% endblock %}
